#include <stdio.h>
int main()
{
  int a=6;
  int b=9;
  printf("a=%d   b=%d\n",a,b);
  a=a+b;//存在整型溢出的可能
  b=a-b;
  a=a-b;
    printf("a=%d   b=%d\n",a,b);
return 0;
}

#include <stdio.h>
int main()
{
int a=6;
int b=9;
  printf("a=%d   b=%d\n",a,b);
  a=a^b;
  b=a^b;
  a=a^b;
    printf("a=%d   b=%d\n",a,b);
return 0;
}
